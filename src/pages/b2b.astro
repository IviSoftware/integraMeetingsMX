---
import InfoPage from "../layouts/InfoPage.astro";

const images = [
  {
    url: "https://raw.githubusercontent.com/Integra-Meetings/integraMeetingsImagenes/main/b2b/conceptualizacion.png",
    alt: "image of conference",
  },
  {
    url: "https://raw.githubusercontent.com/Integra-Meetings/integraMeetingsImagenes/main/b2b/conceptualizacion_2.png",
    alt: "image of conference",
  },
  {
    url: "https://raw.githubusercontent.com/Integra-Meetings/integraMeetingsImagenes/main/b2b/logistica-1-4.png",
    alt: "image of conference",
  },
  {
    url: "https://raw.githubusercontent.com/Integra-Meetings/integraMeetingsImagenes/main/b2b/operacion-1-4.png",
    alt: "image of conference",
  },
  {
    url: "https://raw.githubusercontent.com/Integra-Meetings/integraMeetingsImagenes/main/b2b/planeacion-1-5.png",
    alt: "image of conference",
  },
  {
    url: "https://raw.githubusercontent.com/Integra-Meetings/integraMeetingsImagenes/main/b2b/tecnologia-y-data-1-4.png",
    alt: "image of conference",
  },
];

const headerImageUrl =
  "https://raw.githubusercontent.com/Integra-Meetings/integraMeetingsImagenes/main/b2b.png";
const headerText = `Aprovecha nuestras estrategias de patrocinio y promoción en exposiciones B2B para maximizar tu retorno de inversión. Deja que nuestro equipo se encargue de atraer y retener a los patrocinadores de alto valor, mientras tú te concentras en las prioridades de tu evento.`;
const headerTitle = "Exposiciones B2B";
---

<InfoPage
  title="Exposiciones B2B"
  headerImageUrl={headerImageUrl}
  headerText={headerText}
  headerTitle={headerTitle}
  images={images}
/>
